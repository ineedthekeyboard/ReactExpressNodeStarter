{
  "name": "ExpressNodeStarter",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run cleanWebpack && npm run buildClientProd",
    "buildClientProd": "npm run cleanClient && webpack -p --config webpack.prod.config.js --progress",
    "buildClientDev": "npm run cleanClient && webpack",
    "cleanWebpack": "rimraf node_modules/webpack/node_modules/uglifyjs-webpack-plugin",
    "cleanClient": "rimraf public/*",
    "startDev": "npm run buildClientDev && node --inspect server.js",
    "startProd": "npm run buildClientProd && node server.js",
    "start": "node server.js",
    "uiOnly": "webpack -w"
  },
  "engines": {
    "node": ">=8.1.4"
  },
  "dependencies": {
    "base64url": "^2.0.0",
    "body-parser": "^1.18.2",
    "connect-flash": "^0.1.1",
    "connect-history-api-fallback": "^1.5.0",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "serve-favicon": "^2.4.5",
    "throng": "^4.0.0"
  },
  "devDependencies": {
    "ajv": "^6.0.1",
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.9",
    "es6-promise": "^4.2.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^3.0.6",
    "uglify-es": "^3.3.8",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "react": "^15.5.3",
    "react-dom": "^15.5.3"
  }
}
